{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "name": "article"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "detail": {
      "type": "richtext"
    },
    "published": {
      "type": "boolean"
    },
    "timedate": {
      "type": "datetime"
    },
    "picture": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true
    },
    "category": {
      "model": "category",
      "via": "articles"
    }
  }
}
